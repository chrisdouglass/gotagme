{
  "name": "gotagme",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start": "yarn run server",
    "dev": "nodemon --exec yarn run serverdebug",
    "server": "node ./app.js",
    "serverdebug": "NODE_ENV=development node --inspect ./app.js",
    "snyk": "snyk test",
    "watch": "nodemon -w src --exec 'yarn run check; yarn run compile'",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "prepare": "yarn run compile",
    "pretest": "yarn run compile",
    "posttest": "yarn run check"
  },
  "private": true,
  "dependencies": {
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.9",
    "dotenv": "^5.0.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "flickr-sdk": "^3.6.0",
    "mongoose": "^4.13.11",
    "morgan": "^1.9.0",
    "oauth": "^0.9.15",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "serve-favicon": "^2.4.5",
    "shortid": "^2.2.8",
    "ts-node": "^5.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/mongoose": "^5.0.2",
    "@types/node": "^6.0.60",
    "eslint": "^4.17.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-promise": "^3.6.0",
    "gts": "^0.5.3",
    "jsdoc": "^3.5.5",
    "jsdoc-route-plugin": "^0.1.0",
    "snyk": "^1.69.7",
    "tslint": "^5.9.1",
    "typescript": "2.x",
    "typescript-eslint-parser": "^12.0.0"
  }
}
