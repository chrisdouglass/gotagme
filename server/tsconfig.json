{
  "extends": "./tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "build",
    "noImplicitAny": true
  },
  "include": [
    "src/*.ts",
    "src/**/*.ts",
    "src/*.js",
    "src/**/*.js",
    "test/*.ts",
    "test/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
